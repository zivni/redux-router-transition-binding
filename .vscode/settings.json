// Place your settings in this file to overwrite default and user settings.
{
    "files.exclude": {
		"**/.git": true,
		"**/.DS_Store": true,
        "**/*.js": { "when": "$(basename).tsx"},
        "**/*?.js": { "when": "$(basename).ts"},
        "**/*.map": true
	},
    
    "search.exclude": {
        "**/.git": true,
		"**/bower_components": true,
        "build/" : true
	},
    
    "typescript.tsdk": "node_modules/typescript/lib"
}
